{
    "outputFileName": "input.conf",
    "name": ".Conf Input file configurator",
    "newline": false,
    "properties": {
        
        "Frequency": {
            "title": "Frequency :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 60e6,
            "required": true
        },


        "ExcitationFile": {
            "title": "Excitation File   :",
            "type": "file",
            "htmlType": "text",
            "hint": "Enter the name of your excitation file",
            "validationRule": "nullable|max:255|string",
            "default":"./INPUT/input.exc",
            "required": true,
            "display": "hidden"
        },

        "MaterialFile": {
            "title": "Material File    :",
            "type": "file",
            "htmlType": "text",
            "hint": "Enter the name of your material file",
            "validationRule": "nullable|max:255|string",
            "default":"./INPUT/input.mtr",
            "required": true,
            "display": "hidden"
        },

        "ObservationFileEnable": {
            "title": "Enable Observation File",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "NO",
            "fileDisplay": "NONE",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },


        "ObservationFile": {
            "title": "Observation File  :",
            "type": "file",
            "htmlType": "text",
            "hint": "Enter the name of your observation file",
            "validationRule": "nullable|max:255|string",
            "default":"./INPUT/input.obs",
            "required": true,
            "display": "hidden",
            "displayOnEnable": true,
            "_enabled": {"property": "ObservationFileEnable", "value": "YES"}
        },
        
        "MeshFile": {
            "title": "Mesh file :",
            "type": "file",
            "htmlType": "text",
            "hint": "Enter the name of your mesh file",
            "validationRule": "nullable|max:255|string",
            "default":"Sphere1kfixed.msh",
            "required": true,
            "display": "hidden"
        },

        "InputDirectory": {
            "title": "Input Directory   :",
            "type": "file",
            "htmlType": "text",
            "hint": "Enter the input directory",
            "validationRule": "nullable|max:255|string",
            "default":"./INPUT/Meshes/",
            "required": true,
            "display": "hidden"
        },

        "OutputDirectory": {
            "title": "Output Directory  :",
            "type": "file",
            "htmlType": "text",
            "hint": "Enter the output directory",
            "validationRule": "nullable|max:255|string",
            "default":"./OUTPUT/",
            "required": true,
            "display": "hidden"
        },

        "UseDirectSolver": {
            "title": "Use Direct Solver   :",
            "type": "select",
            "validationRule": "nullable|in:NO,YES",
            "default": "YES",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "SolverType": {
            "title": "Solver Type   :",
            "type": "select",
            "validationRule": "nullable|in:1,0",
            "default": "1",
            "options": [
                {"title": "1 + Direct = LUD", "value": "1"},
                {"title": "0 + Iterative = GMRES", "value": "0"}
            ]
        },

        "BasicType": {
            "title": "Basic Type   :",
            "type": "select",
            "validationRule": "nullable|in:1,0",
            "default": "1",
            "options": [
                {"title": "RGW", "value": "0"},
                {"title": "LCN", "value": "1"}
            ]
        },

        "ForceDenseNearMatrix": {
            "title": "Force Dense Near Matrix   :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "YES",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "ForceSparseNearMatrix": {
            "title": "Force Sparse Near Matrix   :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "NO",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "IterativeSolverTolerance": {
            "title": "Iterative Solver Tolerance     :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 1e-8,
            "required": true
        },

        "CFIEParameter": {
            "title": "CFIE Parameter     :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 1,
            "required": true
        },

        "Solution Order": {
            "title": "Solution Order     :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 1,
            "required": true
        },

        "DisableFMM": {
            "title": "Disable FMM   :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "NO",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "LCNOverride": {
            "title": "LCN Override     :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 0.5,
            "required": true
        },

        "LCNDigits": {
            "title": "LCN Digits     :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 3,
            "required": true
        },

        "FMMDigits": {
            "title": "FMM Digits    :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 6,
            "required": true
        },

        "PreconditionerType": {
            "title": "LCN Override     :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 0,
            "required": true
        },

        "PreconditionerSolverType": {
            "title": "Preconditioner Solver Type     :",
            "type": "number",
	        "htmlType": "number",
            "validationRule": "min:0|numeric",
            "default": 0,
            "required": true
        },

        "QuietOperations": {
            "title": "Quiet Operations    :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "NO",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "VerboseOperations": {
            "title": "Verbose Operations    :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "YES",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "VerifyFMMMode": {
            "title": "Verify FMM Mode    :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "NO",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "Dry Run FMM Mode": {
            "title": "Dry Run FMM Mode    :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "NO",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        },

        "Verify MVP": {
            "title": "Verify MVP    :",
            "type": "select",
            "validationRule": "nullable|in:YES,NO",
            "default": "NO",
            "options": [
                {"title": "YES", "value": "YES"},
                {"title": "NO", "value": "NO"}
            ]
        }

    }
}
